{
  "functions": {
    "rai_advanced.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "rai_advanced.py"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|css|js|webmanifest))$",
      "dest": "/$1",
      "headers": {
        "cache-control": "s-maxage=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
