{
  "version": 2,
  "builds": [
    {
      "src": "rai_advanced.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "factsh.jpeg",
      "use": "@vercel/static"
    },
    {
      "src": "images/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "rai_advanced.py"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp))$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
